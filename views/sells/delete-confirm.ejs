<div class="management">
<% if(title) { %>
  <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>
  <% if(errors) { %>
  <ul class="errors">
    <% errors.array().forEach(error => { %>
    <li><%= error.msg %></li>
    <% }) %>
  </ul>
  <% } %>
  <%- messages() %>

  <p>Are you sure you want to delete this sell? This action cannot be undone.</p>

  <form action="/sells/delete" method="post" enctype="multipart / form-data" id="edit-inventory-form" validate>
    <input type="text" name="account_name" id="account_name" value="<%= account_name %>" readonly>
    <input type="text" name="inv_make" id="inv_make" value="<%- inv_make %>" readonly>
    <input type="text" name="inv_model" id="inv_model" value="<%- inv_model %>" readonly>
    <input type="text" name="inv_price" id="inv_price" value="$<%- inv_price %>" readonly>
      <input type="hidden" name="sell_id"
      <% if(locals.sell_id) { %> value="<%= locals.sell_id %>" <% } %>>
    <input type="submit" value="Delete Sell">
  </form>
</div>